/* eslint-disable */
import type Node from 'react';

declare type HasChildren = {
    +children?: Node;
}

declare type User = {
    id: number;
    username: string;
    full_name: string;
    email: string;
    avatar_url: string;
    is_admin: boolean;
}

declare type UserExtra = User & {
    is_organization: boolean;
    home_link: string;
}

declare type Repo = {
    id: number;
    name: string;
    description: string;
    full_name: string;
    owner: User;
    html_url: string;
    stars_count: number;
    created_at: string;
    updated_at: string;
}

declare type UserActivity = {
    id: number;
    actUser: UserExtra;
    avatar: string;
    opType: string;
    repoLink: string;
    time: string;
    message: string;
    content: UserActivityCommit[] | UserActivityIssueComment | string | null;
};

declare type UserActivityCommit = {
    avatar: string;
    hash: string;
    link: string;
    message: string;
}

declare type UserActivityIssueComment = {
    title: string;
    content: string;
}

declare type UserOrganization = {
    id: number;
    name: string;
    full_name: string;
    home_link: string;
    avatar_url: string;
    description: string;
    num_repos: string;
    website: string;
}

declare var GlobalData: {
    baseUri: string;
    appName: string;
    title: string;
    lang: string;
    isAdmin: boolean;
    isSignedIn: boolean;
    authUser: UserExtra;
    contextUser: UserExtra;
    translations: {
        [key: string]: string;
    };
};
